        "import re\n",
        "def text_match(text):\n",
        "  patterns='[A-Z]+[a-z]'\n",
        "  if re.search(patterns,text):\n",
        "    return \"found a match\"\n",
        "  else:\n",
        "    return(\"not match!\")\n",
        "print(text_match('AaBbCc'))\n",
        "print(text_match('PYTHON'))\n",
        "print(text_match('aA'))\n",
        "print(text_match('Rr'))"
      ],
